@model Projet_Jeu_Role.Models.Situation
				
<br />
<dl class="row">
				<dt class="col-sm-2">
								@Html.DisplayFor(m => m.SituationName)
								<br/>
								@Html.DisplayFor(m => m.SituationDescription)
					</dt>
					<dt class="col-sm-2">
								@{
												for(int i =0; i < Model.AnswerEnter.Count; i++){
																<input type="button" value="@Html.DisplayFor(m=> m.AnswerEnter[i].AnswerContent)" asp-action="Next"/>
												}
								}
					</dt>
</dl>